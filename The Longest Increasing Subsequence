#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int LIS(int arr[],int m)
    {
    int i,max=0,j;
   int lis[m];
    //printf(" \n %d",m);
   for(i=0;i<m;i++)
       lis[i]=1;
    
    for(i=1;i<m;i++)
       {
        for(j=0;j<i;j++)
        {
       if(arr[i]>arr[j]&&lis[i]<lis[j]+1)          
            lis[i]=lis[j]+1;
       }
   }
   for (i = 0; i < m; i++ )
        if (max < lis[i])
            max = lis[i];
   
     
   return max;
}


int main() {
int n,i;
    scanf("%d",&n);
   int arr[n];
    for(i=0;i<n;i++)
        scanf("%d",&arr[i]);
    int l=LIS(arr,n);
    printf("%d",l);
    return 0;
}
