#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int max(int a, int b)
{
    return (a > b)? a : b;
}
  
int lcs( int X[], int Y[], int m, int n )
{
    int i, j;
  /*  for(i=0;i<n;i++)
printf("%d",X[i]);
    printf("\n");
for(i=0;i<m;i++)
printf("%d",Y[i]);*/

   int L[m+1][n+1];
   
  
   /* Following steps build L[m+1][n+1] in bottom up fashion. Note 
      that L[i][j] contains length of LCS of X[0..i-1] and Y[0..j-1] */
   for (i=0; i<=m; i++)
   {
     for (j=0; j<=n; j++)
     {
       if (i == 0 || j == 0)
         L[i][j] = 0;
  
       else if (X[i-1] == Y[j-1])
         L[i][j] = L[i-1][j-1] + 1;
  
       else
         L[i][j] = max(L[i-1][j], L[i][j-1]);
     }
   }
    
   /* L[m][n] contains length of LCS for X[0..n-1] and Y[0..m-1] */
   return L[m][n];
}
int main()
    {
int m,n,i;
  
   scanf("%d %d",&n,&m);
 int P[m],T[n];

for(i=0;i<n;i++)
scanf("%d",&T[i]);
for(i=0;i<m;i++)
scanf("%d",&P[i]);




 /*char *T = "ABABDABACDABABCABAB";
   char *P = "ABABCABAB";*/
int l=lcs(T,P,m,n); 
printf("%d",l);
return 0;
    	
}
